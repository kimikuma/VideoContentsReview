<div class="col-md-2 col-10 offset-1 offset-md-0 search_list mt-3 mt-mb-0 tag_menu">
  <i class="fa-solid fa-list"></i>tag</br>
   <% tags.each do |tag| %>
    <%=link_to tag.name, search_tag_path(tag: tag.name) %></br>
   <% end %>
   <i class="fa-solid fa-list"></i>vod</br>
   <% vods.each do |vod| %>
    <%=link_to vod.name, search_vod_path(vod: vod.name) %></br>
   <% end %>  
     <span class="tag_menu_list"> >> <%=link_to "一覧はこちら", tags_path %></span>
   
</div>